{
  "name": "youkeepfit-backend",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy:dev": "npx serverless deploy --stage dev",
    "deploy:prod": "npx serverless deploy --stage prod",
    "start:local": "serverless offline start --stage local",
    "install:plugin:sls-offline": "serverless plugin install -n serverless-offline --stage local",
    "install:plugin:sls-dynamodb-local": "serverless plugin install -n serverless-dynamodb-local --stage local",
    "install:package:dynamodb": "serverless dynamodb install --stage local"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^3.4.0",
    "@middy/http-json-body-parser": "^3.4.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.1"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "esbuild": "^0.14.11",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^12.0.4",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
